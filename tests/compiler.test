	vexpr {r={ }; for x=1:20 { if x>3 && x<5 || x>17 { r=vstack(r,x) }}; r}
	
} -result {4 18 19 20}

test compiler boolif-3 -body {
	
	vexpr {r={ }; for x=1:20 { if x>=3 && (x<=5 || x>=17) { r=vstack(r,x) }}; r}
	
} -result {3 4 5 17 18 19 20}

test compiler boolif-4 -body {
	
	vexpr {r={ }; for x=1:20 { if x>=3 { r=vstack(r,x) }}; r}
	
